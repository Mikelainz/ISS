plan(3,work, sentence(true,msgswitch(controlMessage, controlMessage( "gameOver") , move( switchplan(gameOver) )),'','' ))
plan(4,work, sentence(true,repeatplan() ,'','' ))
plan(1,pattern, sentence(true,move(actorOp,switchLedState,0,""),'','' ))

plan(5,work, sentence(true,move(repeatplan) ,'','' ))
plan(1,startTo, sentence(true,move(print,"timer iniziato"),'','' ))
plan(2,startTo, sentence(true,receiveMsg(''),'','' ))
plan(3,startTo, sentence(true,move(print,"timer finito"),'','' ))

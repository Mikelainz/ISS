plan(4,work, sentence(true,repeatplan() ,'','' ))
plan(1,startingGame, sentence(true,forward(flashled, controlMessage,controlMessage( "stopFlash")),'','' ))
plan(2,startingGame, sentence(true,move(actorOp,resetStats,0,""),'','' ))

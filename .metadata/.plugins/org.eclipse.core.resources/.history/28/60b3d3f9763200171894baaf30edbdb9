plan(1,init, sentence(true,move(print,mycontrol( starts)),'','' ))
plan(2,init, sentence(true,move( switchplan(work) ),'','' ))
plan(1,work, sentence(true,senseEvent(60000,"buttonEvent","continue"),'','' ))

plan(5,game, sentence(true,move(actorOp,halveDTthreshold,0,""),'','' ))
plan(6,game, sentence(-  actorOpDone( OP,DTthreshold) ,senseEvent(Integer.parseInt(substituteVars(guardVars,"DTthreshold").replace("'","") ),"buttonEvent","continue"),'','' ))
plan(7,game, sentence(true,eventswitch(buttonEvent, »buttonEvent( ButtonPressed), addRule( buttonPressed( ButtonPressed) ) ),'','' ))
plan(8,game, sentence( testOk( B,L),move( switchplan(rightAnswer) ),'','' ))
plan(9,game, sentence(-  tout( R,W) ,move(print,"timed out"),'','' ))
